{
  "name": "solana-bundler",
  "version": "0.1.0",
  "description": "Automated Solana token launcher with Jito bundles for atomic execution",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "solana-bundler": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "solana",
    "jito",
    "bundle",
    "pumpfun",
    "token-launch",
    "mev",
    "bundler",
    "crypto",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.0",
    "bs58": "^5.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "tweetnacl": "^1.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dezintegro/solana-bundler.git"
  },
  "bugs": {
    "url": "https://github.com/dezintegro/solana-bundler/issues"
  },
  "homepage": "https://github.com/dezintegro/solana-bundler#readme"
}
